{
    "name": "kubpro/installer",
    "description": "Laravel project easy installer",
    "type": "library",
    "require": {
        "illuminate/support": "^5.6",
        "php": "^7.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Kubpro\\Installer\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kubpro\\Installer\\Providers\\InstallerServiceProvider"
            ]
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Rauf Abbas",
            "email": "rafo.abbas@gmail.com"
        }
    ],
    "minimum-stability": "stable",

    "prefer-stable": true

}
